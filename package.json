{
  "name": "ilium",
  "version": "1.5.0",
  "description": "the artist network",
  "main": "main.js",
  "engines": {
    "node": "8.9.4",
    "npm": "5.8.0"
  },
  "scripts": {
    "postinstall": "npm run build",
    "clean": "rm -rf ./dist && rm -rf ./public",
    "build": "tsc -p ./server && gulp pre-process && webpack --env.production --mode production",
    "go": "concurrently \"webpack --watch\" \"npm run start\"",
    "start": "node ./dist/server/server.js",
    "tsc": "tsc",
    "tests": "jest",
    "test-coverage": "jest --coverage",
    "test-watch": "jest --watch"
  },
  "dependencies": {
    "awilix": "^3.0.9",
    "aws-sdk": "^2.259.1",
    "body-parser": "^1.18.3",
    "bourbon": "^4.3.4",
    "bourbon-neat": "^2.1.0",
    "classlist.js": "^1.1.20150312",
    "cookie-parser": "^1.4.3",
    "core-js": "^2.5.7",
    "cors": "^2.8.4",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "express-domain-middleware": "^0.1.0",
    "express-handlebars": "^3.0.0",
    "express-session": "^1.15.6",
    "hammerjs": "^2.0.8",
    "heroku-ssl-redirect": "0.0.4",
    "jquery": "^3.3.1",
    "jsonwebtoken": "^8.3.0",
    "lodash.debounce": "^4.0.8",
    "module-alias": "^2.1.0",
    "moment": "^2.22.2",
    "moment-timezone": "^0.5.20",
    "multer": "^1.3.0",
    "mysql": "^2.15.0",
    "mysql2": "^1.5.3",
    "ng-inline-svg": "^3.4.0",
    "ng2-validation": "^4.2.0",
    "ngx-cookie": "^4.0.2",
    "ngx-dropzone-wrapper": "^5.3.5",
    "passport": "^0.3.2",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "path": "^0.12.7",
    "promise-polyfill": "^8.0.0",
    "q": "^1.5.1",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-redux": "^5.0.7",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.4.0-alpha.1",
    "react-router-redux": "^5.0.0-alpha.9",
    "react-svg": "^5.0.8",
    "redux": "^4.0.0",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^6.2.1",
    "rxjs-compat": "^6.2.1",
    "sanitize-html": "^1.18.2",
    "sendgrid": "^4.10.0",
    "sequelize": "^5.0.0-beta.10",
    "string-strip-html": "^1.4.2",
    "web-animations-js": "^2.3.1",
    "zone.js": "~0.8.18"
  },
  "repository": {
    "type": "git",
    "url": "git+https://tcmoore@bitbucket.org/ilium/ilium.git"
  },
  "author": "",
  "license": "ISC",
  "homepage": "https://bitbucket.org/ilium/ilium#readme",
  "devDependencies": {
    "@types/body-parser": "0.0.33",
    "@types/chance": "^0.7.36",
    "@types/cors": "^2.8.4",
    "@types/express": "^4.16.0",
    "@types/glob": "^5.0.35",
    "@types/googlemaps": "^3.30.10",
    "@types/history": "^4.6.2",
    "@types/jest": "^21.1.10",
    "@types/jquery": "^2.0.49",
    "@types/jsonwebtoken": "^7.2.7",
    "@types/moment-timezone": "^0.2.35",
    "@types/multer": "0.0.33",
    "@types/mysql": "0.0.31",
    "@types/node": "^6.0.113",
    "@types/passport": "^0.3.5",
    "@types/q": "^1.5.0",
    "@types/react": "^16.4.7",
    "@types/react-dom": "^16.0.6",
    "@types/react-redux": "^6.0.5",
    "@types/react-router": "^4.0.29",
    "@types/react-router-dom": "^4.3.0",
    "@types/react-router-redux": "^5.0.15",
    "@types/sendgrid": "^2.0.31",
    "@types/sequelize": "^4.27.24",
    "@types/webpack": "^3.8.12",
    "browser-sync": "^2.24.4",
    "browser-sync-webpack-plugin": "^2.2.2",
    "chance": "^1.0.16",
    "concurrently": "^3.6.1",
    "copy-webpack-plugin": "^4.5.1",
    "css-loader": "^0.28.11",
    "del": "3.0.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "html-webpack-plugin": "^3.2.0",
    "ignore-loader": "^0.1.2",
