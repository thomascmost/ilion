    "jest-preset-angular": "^5.2.2",
    "node-sass": "^4.9.0",
    "nodemon-browsersync-webpack-plugin": "^1.0.1",
    "progress-bar-webpack-plugin": "^1.11.0",
    "sass-loader": "^6.0.7",
    "sequelize-typescript": "^0.6.6-beta.1",
    "style-loader": "^0.18.2",
    "ts-helpers": "^1.1.2",
    "ts-jest": "^22.4.6",
    "ts-loader": "^4.4.2",
    "ts-node": "^3.3.0",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "tslint": "^4.5.1",
    "typescript": "^2.9.2",
    "webpack": "^4.16.3",
    "webpack-build-logger": "0.0.5",
    "webpack-cli": "^2.1.5",
    "webpack-stream": "^4.0.3",
    "webpackbar": "^2.6.1"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "tsConfigFile": "./tsconfig.spec.json"
      }
    },
    "setupTestFrameworkScriptFile": "<rootDir>/setup-jest.ts",
    "transform": {
      "^.+\\.ts?$": "ts-jest"
    },
    "testMatch": [
      "**/+(*.)+(spec|test).+(ts)?(x)"
    ],
    "moduleNameMapper": {
      "@ilium/models(.*)$": "<rootDir>/shared/models/$1",
      "@ilium/effects(.*)$": "<rootDir>/shared/client/effects/$1",
      "@ilium/stores(.*)$": "<rootDir>/shared/client/stores/$1",
      "@ilium/shared(.*)$": "<rootDir>/shared/$1",
      "@ilium/services(.*)$": "<rootDir>/shared/services/$1",
      "@ilium/mocks(.*)$": "<rootDir>/shared/test/$1",
      "@ilium/env(.*)$": "<rootDir>/web/$1"
    },
    "moduleFileExtensions": [
      "ts",
      "js",
      "json"
    ]
  },
  "_moduleAliases": {
    "@ilium/models": "dist/shared/models",
    "@ilium/shared": "dist/shared"
  }
}
